    "def get_summ_digits():\n",
    "    summ_digits = 0\n",
    "    \n",
    "    def summix(digit):\n",
    "        nonlocal summ_digits\n",
    "        summ_digits += digit\n",
    "        return summ_digits\n",
    "    \n",
    "    return summix\n",
    "\n",
    "\n",
    "ff = get_summ_digits()\n",
    "\n",
    "print (ff (2))\n",
    "print (ff (7))\n",
    "print (ff (5))"
   ]
